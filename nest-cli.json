{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/apps/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/apps/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/apps",
  "projects": {
    "apps": {
      "type": "application",
      "root": "apps/apps",
      "entryFile": "main",
      "sourceRoot": "apps/apps/src",
      "compilerOptions": {
        "tsConfigPath": "apps/apps/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "sub-task": {
      "type": "application",
      "root": "apps/sub-task",
      "entryFile": "main",
      "sourceRoot": "apps/sub-task/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sub-task/tsconfig.app.json"
      }
    },
    "tag": {
      "type": "application",
      "root": "apps/tag",
      "entryFile": "main",
      "sourceRoot": "apps/tag/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tag/tsconfig.app.json"
      }
    },
    "todo-item": {
      "type": "application",
      "root": "apps/todo-item",
      "entryFile": "main",
      "sourceRoot": "apps/todo-item/src",
      "compilerOptions": {
        "tsConfigPath": "apps/todo-item/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}